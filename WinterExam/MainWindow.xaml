<Window x:Name="Liam_and_Noels_Ticket_Oasis" x:Class="WinterExam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WinterExam"
        mc:Ignorable="d"
        Title="Liam and Noels Ticket Oasis" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstbxEvents" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="79,93,485,93"/>
        <Label Content="Events" HorizontalAlignment="Left" Margin="79,67,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lstbxTickets" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="357,93,96,93"/>
        <Label Content="Tickets" HorizontalAlignment="Left" Margin="357,67,0,0" VerticalAlignment="Top"/>
        <Label Content="Number of Tickets" HorizontalAlignment="Left" Margin="423,346,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxNumTickets" HorizontalAlignment="Left" Margin="535,350,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnBookTicket" Content="Book" HorizontalAlignment="Left" Margin="664,349,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
